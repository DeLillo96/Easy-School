<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" stylesheets="@../Resources/Style/default.css"
            xmlns="http://javafx.com/javafx/8" fx:controller="Client.Controller.ChildrenController">
    <children>
        <VBox alignment="CENTER" layoutY="131.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                      minWidth="-Infinity" prefHeight="110.0" prefWidth="1136.0">
                    <children>
                        <VBox alignment="CENTER" prefHeight="313.0" prefWidth="155.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                                <TextField fx:id="nameTextField">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </TextField>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="155.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Surname">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                                <TextField fx:id="surnameTextField">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </TextField>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="165.0" layoutY="10.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Birth from">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                                <DatePicker fx:id="birthDatePickerFrom">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </DatePicker>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="165.0" layoutY="10.0" prefHeight="200.0" prefWidth="155.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Birth to">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                                <DatePicker fx:id="birthDatePickerTo">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </DatePicker>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" layoutX="165.0" layoutY="10.0" prefHeight="200.0" prefWidth="200.0">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="FiscalCode">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                                <TextField fx:id="fiscalCodeTextField">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </TextField>
                            </children>
                        </VBox>
                        <Button fx:id="searchButton" alignment="CENTER" defaultButton="true" onAction="#filter">
                            <graphic>
                                <ImageView fitHeight="40.0" fitWidth="40.0">
                                    <image>
                                        <Image url="@../Resources/Images/search.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <HBox.margin>
                                <Insets left="5.0"/>
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="alone-button"/>
                                <String fx:value="radius-15"/>
                            </styleClass>
                        </Button>
                        <Button fx:id="addButton" alignment="CENTER" onAction="#add">
                            <graphic>
                                <ImageView fitHeight="40.0" fitWidth="40.0">
                                    <image>
                                        <Image url="@../Resources/Images/add.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <HBox.margin>
                                <Insets left="30.0" right="1.0"/>
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="alone-button"/>
                                <String fx:value="radius-15"/>
                            </styleClass>
                        </Button>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <VBox.margin>
                        <Insets left="10.0"/>
                    </VBox.margin>
                </HBox>
                <TableView fx:id="childTableView" layoutY="131.0" prefWidth="1136.0">
                    <columns>
                        <TableColumn fx:id="buttonsColumn" minWidth="-Infinity" prefWidth="-1.0">
                            <cellValueFactory>
                                <PropertyValueFactory property="buttons"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="nameColumn" minWidth="-Infinity" prefWidth="-1.0" text="Name">
                            <cellValueFactory>
                                <PropertyValueFactory property="name"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="surnameColumn" minWidth="-Infinity" prefWidth="-1.0" text="Surname">
                            <cellValueFactory>
                                <PropertyValueFactory property="surname"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="dateColumn" minWidth="-Infinity" prefWidth="-1.0" text="Birth Date">
                            <cellValueFactory>
                                <PropertyValueFactory property="birthDate"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="fiscalCodeColumn" minWidth="-Infinity" prefWidth="-1.0" text="Fiscal Code">
                            <cellValueFactory>
                                <PropertyValueFactory property="fiscalCode"/>
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0"/>
                    </VBox.margin>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>
