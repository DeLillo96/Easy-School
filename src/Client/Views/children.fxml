  <columns>
    <TableColumn fx:id="nameColumn" prefWidth="152.0" resizable="false" text="Name">
        <cellValueFactory>
            <PropertyValueFactory property="name" />
        </cellValueFactory>
    </TableColumn>
      <TableColumn fx:id="surnameColumn" minWidth="0.0" prefWidth="158.0" text="Surname">
          <cellValueFactory>
              <PropertyValueFactory property="surname" />
          </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="dateColumn" minWidth="5.0" prefWidth="193.0" text="Birth Date">
          <cellValueFactory>
              <PropertyValueFactory property="birthDate" />
          </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="fiscalCodeColumn" prefWidth="220.0" text="Fiscal Code">
          <cellValueFactory>
              <PropertyValueFactory property="fiscalCode" />
          </cellValueFactory>
      </TableColumn>
  </columns>
</TableView>
