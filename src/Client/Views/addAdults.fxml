<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane opacity="0.9" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Controller.AddAdultsController">
   <children>
      <SplitPane dividerPositions="0.5" prefHeight="600.0" prefWidth="900.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <TableView fx:id="actualParentTableView" layoutX="50.0" layoutY="100.0" prefHeight="400.0" prefWidth="350.0">
                    <columns>
                        <TableColumn fx:id="actualParentColumn" minWidth="100.0" prefWidth="-1.0" text="Actual Parents">
                            <cellValueFactory>
                                <PropertyValueFactory property="name" />
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                  </TableView>
                  <Button fx:id="buttonClose" layoutX="27.0" layoutY="14.0" mnemonicParsing="false" onAction="#remove" prefHeight="39.0" prefWidth="46.0" text="X" />
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <TableView fx:id="allParentTableView" layoutX="50.0" layoutY="100.0" prefHeight="400.0" prefWidth="350.0">
                    <columns>
                        <TableColumn fx:id="allParentColumn" minWidth="100.0" prefWidth="-1.0" text="All Parents">
                            <cellValueFactory>
                                <PropertyValueFactory property="name" />
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                  </TableView>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</AnchorPane>
