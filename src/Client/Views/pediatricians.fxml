<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" styleClass="anchor-pane-popup"
            stylesheets="@../Resources/Style/default.css" xmlns="http://javafx.com/javafx/9"
            fx:controller="Client.Controller.PediatricianController">
    <children>
        <AnchorPane AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
            <children>
                <VBox AnchorPane.rightAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                            <children>
                                <VBox alignment="CENTER">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Child in modify =">
                                            <font>
                                                <Font name="Monospaced Regular" size="25.0"/>
                                            </font>
                                        </Text>
                                    </children>
                                </VBox>
                                <VBox alignment="CENTER">
                                    <children>
                                        <Text fx:id="childName" strokeType="OUTSIDE" strokeWidth="0.0">
                                            <font>
                                                <Font name="Monospaced Regular" size="25.0"/>
                                            </font>
                                        </Text>
                                    </children>
                                    <padding>
                                        <Insets left="15.0"/>
                                    </padding>
                                </VBox>
                                <VBox alignment="CENTER" layoutX="165.0" layoutY="10.0">
                                    <children>
                                        <Text fx:id="childSurname" strokeType="OUTSIDE" strokeWidth="0.0">
                                            <font>
                                                <Font name="Monospaced Regular" size="25.0"/>
                                            </font>
                                        </Text>
                                    </children>
                                    <padding>
                                        <Insets left="15.0"/>
                                    </padding>
                                </VBox>
                                <VBox alignment="CENTER" layoutX="165.0" layoutY="10.0">
                                    <children>
                                        <Text fx:id="childFiscalCode" strokeType="OUTSIDE" strokeWidth="0.0">
                                            <font>
                                                <Font name="Monospaced Regular" size="25.0"/>
                                            </font>
                                        </Text>
                                    </children>
                                    <HBox.margin>
                                        <Insets left="15.0"/>
                                    </HBox.margin>
                                </VBox>
                            </children>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <padding>
                                <Insets left="3.0"/>
                            </padding>
                        </HBox>
                        <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0"
                              AnchorPane.topAnchor="0.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0"
                                      AnchorPane.topAnchor="0.0">
                                    <children>
                                        <VBox alignment="CENTER">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
                                                    <font>
                                                        <Font name="Monospaced Regular" size="25.0"/>
                                                    </font>
                                                </Text>
                                                <TextField fx:id="nameTextField">
                                                    <VBox.margin>
                                                        <Insets left="5.0" right="5.0"/>
                                                    </VBox.margin>
                                                </TextField>
                                            </children>
                                        </VBox>
                                        <VBox alignment="CENTER">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Surname">
                                                    <font>
                                                        <Font name="Monospaced Regular" size="25.0"/>
                                                    </font>
                                                </Text>
                                                <TextField fx:id="surnameTextField">
                                                    <VBox.margin>
                                                        <Insets left="5.0" right="5.0"/>
                                                    </VBox.margin>
                                                </TextField>
                                            </children>
                                        </VBox>
                                        <VBox alignment="CENTER" layoutX="165.0" layoutY="10.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Birth from">
                                                    <font>
                                                        <Font name="Monospaced Regular" size="25.0"/>
                                                    </font>
                                                </Text>
                                                <DatePicker fx:id="birthDatePickerFrom">
                                                    <VBox.margin>
                                                        <Insets left="5.0" right="5.0"/>
                                                    </VBox.margin>
                                                </DatePicker>
                                            </children>
                                        </VBox>
                                        <VBox alignment="CENTER" layoutX="165.0" layoutY="10.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Birth to">
                                                    <font>
                                                        <Font name="Monospaced Regular" size="25.0"/>
                                                    </font>
                                                </Text>
                                                <DatePicker fx:id="birthDatePickerTo">
                                                    <VBox.margin>
                                                        <Insets left="5.0" right="5.0"/>
                                                    </VBox.margin>
                                                </DatePicker>
                                            </children>
                                        </VBox>
                                        <VBox alignment="CENTER" layoutX="165.0" layoutY="10.0">
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="FiscalCode">
                                                    <font>
                                                        <Font name="Monospaced Regular" size="25.0"/>
                                                    </font>
                                                </Text>
                                                <TextField fx:id="fiscalCodeTextField">
                                                    <VBox.margin>
                                                        <Insets left="5.0" right="5.0"/>
                                                    </VBox.margin>
                                                </TextField>
                                            </children>
                                        </VBox>
                                        <Button fx:id="searchButton" alignment="CENTER" defaultButton="true"
                                                onAction="#filter">
                                            <tooltip>
                                                <Tooltip text="Search"/>
                                            </tooltip>
                                            <graphic>
                                                <ImageView fitHeight="40.0" fitWidth="40.0">
                                                    <image>
                                                        <Image url="@../Resources/Images/search.png"/>
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                            <HBox.margin>
                                                <Insets left="5.0"/>
                                            </HBox.margin>
                                            <styleClass>
                                                <String fx:value="alone-button"/>
                                                <String fx:value="radius-15"/>
                                            </styleClass>
                                        </Button>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_RIGHT" AnchorPane.bottomAnchor="0.0"
                                      AnchorPane.rightAnchor="0.0"
                                      AnchorPane.topAnchor="0.0">
                                    <children>
                                        <Button fx:id="addButton" alignment="CENTER" onAction="#add">
                                            <tooltip>
                                                <Tooltip text="Add new adult"/>
                                            </tooltip>
                                            <graphic>
                                                <ImageView fitHeight="40.0" fitWidth="40.0">
                                                    <image>
                                                        <Image url="@../Resources/Images/add.png"/>
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                            <HBox.margin>
                                                <Insets left="30.0" right="1.0"/>
                                            </HBox.margin>
                                            <styleClass>
                                                <String fx:value="alone-button"/>
                                                <String fx:value="radius-15"/>
                                            </styleClass>
                                        </Button>
                                    </children>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </HBox>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
        <TableView fx:id="tableView" prefWidth="1136.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
                   AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="130.0">
            <columns>
                <TableColumn fx:id="buttonsColumn" minWidth="-Infinity" prefWidth="-1.0">
                    <cellValueFactory>
                        <PropertyValueFactory property="buttons"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="checkPediatricianColumn" minWidth="-Infinity" prefWidth="0.0" text="Pediatrician">
                    <cellValueFactory>
                        <PropertyValueFactory property="select"/>
                    </cellValueFactory>
                    <styleClass>
                        <String fx:value="table-cell"/>
                    </styleClass>
                </TableColumn>
                <TableColumn fx:id="nameColumn" minWidth="-Infinity" prefWidth="-1.0" text="Name">
                    <cellValueFactory>
                        <PropertyValueFactory property="name"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="surnameColumn" minWidth="-Infinity" prefWidth="-1.0" text="Surname">
                    <cellValueFactory>
                        <PropertyValueFactory property="surname"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="dateColumn" minWidth="-Infinity" prefWidth="-1.0" text="Birth Date">
                    <cellValueFactory>
                        <PropertyValueFactory property="birthDate"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="fiscalCodeColumn" minWidth="-Infinity" prefWidth="-1.0" text="Fiscal Code">
                    <cellValueFactory>
                        <PropertyValueFactory property="fiscalCode"/>
                    </cellValueFactory>
                </TableColumn>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Button alignment="TOP_RIGHT" mnemonicParsing="false" onAction="#remove" text="X" AnchorPane.rightAnchor="3.0"
                AnchorPane.topAnchor="3.0">
            <tooltip>
                <Tooltip text="Remove current popup"/>
            </tooltip>
            <styleClass>
                <String fx:value="circle-button"/>
                <String fx:value="orange-button"/>
            </styleClass>
        </Button>
    </children>
</AnchorPane>