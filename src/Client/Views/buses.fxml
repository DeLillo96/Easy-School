<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" styleClass="anchor-pane-popup"
            stylesheets="@../Resources/Style/default.css" xmlns="http://javafx.com/javafx/9"
            fx:controller="Client.Controller.BusesController">
    <children>
        <VBox alignment="CENTER" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox alignment="CENTER_LEFT" spacing="5.0">
                    <children>
                        <VBox alignment="CENTER">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add arrival buses to place =">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER">
                            <children>
                                <Text fx:id="placeName" strokeType="OUTSIDE" strokeWidth="0.0">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                            </children>
                            <padding>
                                <Insets left="15.0"/>
                            </padding>
                        </VBox>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets left="3.0"/>
                    </padding>
                </HBox>
                <HBox alignment="CENTER_LEFT" spacing="5.0">
                    <children>
                        <VBox alignment="CENTER">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="License Plate">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                                <TextField fx:id="licensePlateTextField">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </TextField>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Company Name">
                                    <font>
                                        <Font name="Monospaced Regular" size="25.0"/>
                                    </font>
                                </Text>
                                <TextField fx:id="companyNameTextField">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0"/>
                                    </VBox.margin>
                                </TextField>
                            </children>
                        </VBox>
                        <Button fx:id="searchButton" alignment="CENTER" defaultButton="true" onAction="#filter">
                            <graphic>
                                <ImageView fitHeight="40.0" fitWidth="40.0">
                                    <image>
                                        <Image url="@../Resources/Images/search.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <HBox.margin>
                                <Insets left="5.0"/>
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="alone-button"/>
                                <String fx:value="radius-15"/>
                            </styleClass>
                        </Button>
                        <Button fx:id="addButton" alignment="CENTER" onAction="#add">
                            <graphic>
                                <ImageView fitHeight="40.0" fitWidth="40.0">
                                    <image>
                                        <Image url="@../Resources/Images/add.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <HBox.margin>
                                <Insets left="30.0" right="1.0"/>
                            </HBox.margin>
                            <styleClass>
                                <String fx:value="alone-button"/>
                                <String fx:value="radius-15"/>
                            </styleClass>
                        </Button>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                    <padding>
                        <Insets left="3.0"/>
                    </padding>
                </HBox>
                <TableView fx:id="busesTableView" layoutY="131.0">
                    <columns>
                        <TableColumn fx:id="buttonsColumn" maxWidth="4000.0" minWidth="-Infinity" prefWidth="-1.0">
                            <cellValueFactory>
                                <PropertyValueFactory property="buttons"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="checkStartBusColumn" maxWidth="3000.0" minWidth="-Infinity"
                                     prefWidth="0.0" text="departure">
                            <cellValueFactory>
                                <PropertyValueFactory property="startSelect"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="checkArrivalBusColumn" maxWidth="3000.0" minWidth="-Infinity"
                                     prefWidth="0.0" text="arrivals">
                            <cellValueFactory>
                                <PropertyValueFactory property="arrivalSelect"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="licensePlateColumn" minWidth="-Infinity" prefWidth="-1.0"
                                     text="License Plate">
                            <cellValueFactory>
                                <PropertyValueFactory property="licensePlate"/>
                            </cellValueFactory>
                        </TableColumn>
                        <TableColumn fx:id="companyNameColumn" minWidth="-Infinity" prefWidth="-1.0"
                                     text="Company Name">
                            <cellValueFactory>
                                <PropertyValueFactory property="companyName"/>
                            </cellValueFactory>
                        </TableColumn>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0"/>
                    </VBox.margin>
                </TableView>
            </children>
            <padding>
                <Insets top="30.0"/>
            </padding>
        </VBox>
        <Button alignment="TOP_RIGHT" mnemonicParsing="false" onAction="#remove" text="X" AnchorPane.rightAnchor="3.0"
                AnchorPane.topAnchor="3.0">
            <styleClass>
                <String fx:value="circle-button"/>
                <String fx:value="exit-button"/>
            </styleClass>
        </Button>
    </children>
</AnchorPane>
